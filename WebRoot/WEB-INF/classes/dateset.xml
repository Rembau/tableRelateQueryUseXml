<?xml version="1.0" encoding="UTF-8"?>
<datesets>
	<device>SELECT d.ip_addr,d.device_id FROM rm.rm_device d,rm.rm_device_template t where d.SYS_OID=t.SYS_OID and (t.device_type='SWITCH'or t.SYS_MODEL='ME60') </device>
	<dateset id="101">
		<table name ="nhm.nhm_cpu_mem_kpi">
			<column field="IP_ADDR" name="设备IP"></column>
			<column field="ONLINE_STATUS" name="在线状态" force-value=""></column>
			<column field="CPU_UTILTY" name="CPU占用率"></column>
			<column field="MEM_UTILTY" name="内存占用率"></column>
		</table>
	</dateset>
	<dateset id="102">
		<table name="rm.rm_interface"> 
			<column field="IP_ADDR" name="设备IP"></column>
			<column field="IF_NAME" name="接口名称"></column>
			<column field="IF_ADMIN_STATUS" name="管理状态"></column>
			<column field="IF_OPER_STATUS" name="运行状态"></column>
		</table>
		<table name="nhm.nhm_cpu_mem_kpi">
			<column field="IF_IN_TRAFFIC" name="接收速率(bps/秒)"></column>
			<column field="IF_OUT_TRAFFIC" name="发送速率(bps/秒)"></column>
			<column field="IN_ERROR_PACKAGE" name="接收错误包数"></column>
			<column field="OUT_ERROR_PACKAGE" name="发送错误包数"></column>
			<column field="IN_DISCARDS_PACKAGE" name="接收丢包数" force-value="0"></column>
			<column field="OUT_DISCARDS_PACKAGE" name="发送丢包数" force-value="0"></column>
			<column field="IF_IN_DISCARDS" name="接收丢包率" force-value="0"></column>
			<column field="IF_OUT_DISCARDS" name="发送丢包率" force-value="0"></column>
		</table>
	</dateset>
</datesets>